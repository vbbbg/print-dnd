{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 60,
                "memory": 1024
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/src/index.js"
        }
    ]
}